# OpenAI API
# Получить API ключ: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Database (Supabase PostgreSQL)
# Найти в Supabase Dashboard: Settings > Database > Connection string
# Используйте Direct Connection (для development) или Connection Pooling (для production)
# Замените [YOUR-PASSWORD] на пароль вашей базы данных
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.your-project.supabase.co:5432/postgres

# Supabase
# Найти в Supabase Dashboard: Settings > API
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here